<ul>
  {{#each roomsFromDB}}
  <li> 
    <h3><a href="/rooms/{{this._id}}"> {{this.name }} </a></h3> 
    <img src="{{this.imageUrl}}" alt="" width="300">
    <br>
    {{#if this.isOwner}}
      <form action="/rooms/{{this._id}}/delete" method="POST">
        <button type="submit">Delete</button>
      </form>
      <br>
      <a href="/rooms/{{this._id}}/edit" class="edit-button">EDIT</a>
    {{/if}}
  </li>
  {{/each}}
</ul>
